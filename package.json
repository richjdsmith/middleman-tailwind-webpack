    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-purgecss": "^1.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
